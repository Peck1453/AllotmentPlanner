
@{
    ViewBag.Title = "GetRolesForUserConfirmed";
}

<h2>Roles for this User are as follows:</h2>
@if (ViewBag.RolesForThisUser != null)
{
    <div>
        <ol>
            @foreach (string s in ViewBag.RolesForThisUser)
            {
                <li>@s</li>
            }

        </ol>
    </div>
}
@Html.ActionLink("Back to Users", "GetUsers", "Admin")<br />

@Html.ActionLink("Back to Dashboard", "AdminDashboard", "Home")




